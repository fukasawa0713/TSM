<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>ServletTest</servlet-name>
    <servlet-class>jp.main.servlet.ServletTest</servlet-class>
  </servlet>



  <servlet>
    <servlet-name>TeacherSearch</servlet-name>
    <servlet-class>jp.main.servlet.TeacherSearch</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>TeacherUpdate</servlet-name>
    <servlet-class>jp.main.servlet.TeacherUpdate</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>TeacherInsert</servlet-name>
    <servlet-class>jp.main.servlet.TeacherInsert</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>ServletTest</servlet-name>
    <url-pattern>/ServletTest</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>TeacherSearch</servlet-name>
    <url-pattern>/servlet/SearchServlet</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>TeacherUpdate</servlet-name>
    <url-pattern>/servlet/UpdateServlet</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>TeacherInsert</servlet-name>
    <url-pattern>/servlet/InsertServlet</url-pattern>
  </servlet-mapping>
</web-app>
