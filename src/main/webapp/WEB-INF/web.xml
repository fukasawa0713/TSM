<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>TeacherInfoServlet</servlet-name>
    <servlet-class>jp.main.servlet.TeacherInfoServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>TeacherSearchServlet</servlet-name>
    <servlet-class>jp.main.servlet.TeacherSearchServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>TeacherUpdateServlet</servlet-name>
    <servlet-class>jp.main.servlet.TeacherUpdateServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UpdateSearchServlet</servlet-name>
    <servlet-class>jp.main.servlet.UpdateSearchServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>TeacherInsertServlet</servlet-name>
    <servlet-class>jp.main.servlet.TeacherInsertServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>TeacherCountServlet</servlet-name>
    <servlet-class>jp.main.servlet.TeacherCountServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>CountServlet</servlet-name>
    <servlet-class>jp.main.servlet.CountServlet</servlet-class>
  </servlet>


  <servlet-mapping>
    <servlet-name>TeacherCountServlet</servlet-name>
    <url-pattern>/TeacherCountServlet</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>CountServlet</servlet-name>
    <url-pattern>/CountServlet</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>TeacherSearchServlet</servlet-name>
    <url-pattern>/TeacherSearchServlet</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>TeacherUpdateServlet</servlet-name>
    <url-pattern>/TeacherUpdateServlet</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>TeacherInsertServlet</servlet-name>
    <url-pattern>/TeacherInsertServlet</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>UpdateSearchServlet</servlet-name>
    <url-pattern>/UpdateSearchServlet</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>TeacherInfoServlet</servlet-name>
    <url-pattern>/TeacherInfoServlet</url-pattern>
  </servlet-mapping>

</web-app>
